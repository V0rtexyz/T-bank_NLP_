# LLM провайдер
LLM_PROVIDER=chatgpt

# Retriever API настройки
RETRIEVER_API_URL=http://localhost:8010
RETRIEVER_API_TIMEOUT=60.0

# Redis настройки для хранения истории диалогов
# Внутри Docker сети Redis слушает на стандартном порту 6379
# Внешний порт хоста: 6389 (маппится на внутренний 6379)
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=
SESSION_TTL=86400
MAX_HISTORY_MESSAGES=100
